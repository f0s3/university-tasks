#include <iostream>
#include <cmath>
#include <stdio.h>

using namespace std;

int main() {

    double x = 0;
    double s = 0;

    while (x <= 2) {

        double f1 = 20 - 2 * pow(x, 2) - 6 * x;
        double f2 = 0;
        s += s + (f2 - f1);
        cout << s << endl;
        x += 0.33;
    }

    return 0;
}
